<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600" prefWidth="800" styleClass="vbox" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.condo.controllers.etcController.FinishRePasswordController">
   <children>
      <Label layoutX="232.0" layoutY="163.0" styleClass="text5" text="เปลี่ยนรหัสผ่านเสร็จสมบูรณ์" textFill="#bd9b76">
         <font>
            <Font name="Calibri Bold" size="42.0" />
