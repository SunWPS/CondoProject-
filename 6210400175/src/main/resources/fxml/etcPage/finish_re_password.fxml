         </graphic>
      </Button>
      <Button fx:id="logoutBtn" layoutX="187.0" layoutY="368.0" mnemonicParsing="false" onAction="#handleLogoutBtnOnAction" prefHeight="78.0" prefWidth="184.0" styleClass="cancelButton" text="ออกจากระบบ" textFill="WHITE">
